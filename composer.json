{
  "name": "assure/workflow",
  "description": "Workflow package for Laravel 5.5",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.0",
    "illuminate/support": "5.5.*",
    "illuminate/database": "5.5.*",
    "illuminate/contracts": "5.5.*",
    "illuminate/routing": "5.5.*",
    "illuminate/view": "5.5.*"
  },
  "autoload": {
    "psr-4": {
      "Assure\\Workflow\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Assure\\Workflow\\WorkflowServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5"
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
